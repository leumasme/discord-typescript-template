{
  "name": "typescript-template",
  "version": "1.0.0",
  "description": "A Template for a generic TypeScript project",
  "main": "build/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node build/index.js",
    "launch": "npm run build && npm run start",
    "prepare": "npx prisma migrate dev --preview-feature",
    "clean": "del build /Q /S",
    "clean:db": "prisma migrate reset --preview-feature",
    "rebuild": "npm run clean && npm run prepare && npm run build"
  },
  "author": "Temm",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.34",
    "@types/ws": "^7.4.0",
    "prisma": "^2.20.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0"
  }
}
